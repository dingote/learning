k=1

for i in range(1,101):
    print(i,end=' ')
    div=[]
    if i % 2 == 0:
        div.append(2)
    if i % 3 == 0:
        div.append(3)
    if i % 5 == 0:
        div.append(5)
    if len(div) == 0:
        div=""
    else:
        div=" "+str(tuple(div)).replace(",)",")")

    if k%3 == 0 :
        print(div, end='\n')
    else:
        print(div, end='\t\t\t')
    k = k+1
